a=[10,11,"Zakir","Bala",123,44,53,"Anuj",20,46,7,"Jack"]
odd=[]
even=[]
names=[]
for i in a:
    if(type(i) is int):
        if(i%2==1):
            odd.append(i)
        elif(i%2==0):
            even.append(i)
    else:
        names.append(i)
    odd.sort()
    even.sort()
    names.sort()
print(odd+even+names)

        